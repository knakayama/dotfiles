[user]
	name = knakayama
	email = knakayama.sh@gmail.com
[alias]
	st = status
	sts = status -s
	co = commit
	com = commit -m
	coam = commit -am
	l = log --graph --oneline --all --branches --decorate
[color]
	ui = auto
[grep]
	lineNumber = true
[github]
	user = knakayama
[init]
	templatedir = ~/.gittemplates
[color "status"]
	added = green bold
	updated = green bold
	changed = red bold
	untracked = red bold
[diff]
	ignoreSubmodules = dirty
