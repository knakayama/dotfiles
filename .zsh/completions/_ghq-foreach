#compdef ghq-foreach

local curcontext="$corcontext" state line ret_val=1
typeset -A opt_args

_arguments -C \
  '(-h)'-h'[Print help message]' \
  '(-g)'-g'[<git-cmd> execute git command]' \
  '(-c)'-c'[<shell-cmd> execute shell command]'

return ret_val

